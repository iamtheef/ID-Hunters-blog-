<%include header%>

<h1 style="font-size: 46px;
	font-weight: lighter; padding-left: 30px;">Login</h1>


<form action="/login" style="margin: auto;" method="post">
	<div class="container col-2">
		<div class="row" style="margin-bottom: 20px;">
			<input type="text" name="username" placeholder="Username" style="min-width: 300px">
		</div>
		<div class="row" style="margin-bottom: 20px;">
			<input type="password" name="password" style="min-width: 300px" placeholder="Password">
		</div>
		<div class="row">
			<button class="btn btn-dark">Login</button>
		</div>
	</div>

</form>





